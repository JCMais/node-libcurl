{
  "name": "node-libcurl",
  "version-string": "$$NODE_LIBCURL_VERSION$$",
  "dependencies": [
    {
      "name": "curl",
      "version>=": "8.16.0",
      "features": [
        "brotli",
        "c-ares",
        "http2",
        "http3",
        "ldap",
        "gsasl",
        "idn",
        "idn2",
        "openssl",
        "ssh",
        "sspi",
        "websockets",
        "zstd",
        "tool"
      ]
    }
  ],
  "overrides": [
    {
      "name": "openssl",
      "version": "$$OPENSSL_VERSION$$"
    }
  ]
}
